{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "cline-desktop",
	"version": "0.1.0",
	"identifier": "com.cline.desktop",
	"build": {
		"frontendDist": "./webview-ui/dist",
		"devUrl": "http://localhost:25463/",
		"beforeDevCommand": "",
		"beforeBuildCommand": "cd webview-ui && npm run build"
	},
	"app": {
		"windows": [
			{
				"title": "Cline Desktop",
				"width": 800,
				"height": 600,
				"resizable": true,
				"fullscreen": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
